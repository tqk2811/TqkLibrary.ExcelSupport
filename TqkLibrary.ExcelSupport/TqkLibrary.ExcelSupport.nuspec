<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<!--https://docs.microsoft.com/en-us/nuget/reference/nuspec-->
	<metadata>
		<id>$id$</id>
		<version>1.0.0-build$buildDay$$buildIndex$</version>
		<description>my private lib</description>
		<authors>tqk2811</authors>
		<projectUrl>https://github.com/tqk2811/TqkLibrary.ExcelSupport</projectUrl>
		<repository type="git" url="https://github.com/tqk2811/TqkLibrary.ExcelSupport.git"/>
		<license type="expression">MIT</license>
		<dependencies>
			<group targetFramework="netstandard2.0">
				<dependency id="EPPlus" version="7.1.3" />
				<dependency id="Nito.AsyncEx" version="5.1.2" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="bin\Release\**\$id$.dll" target="lib\" />
		<file src="bin\Release\**\$id$.pdb" target="lib\" />
		<file src="bin\Release\**\$id$.xml" target="lib\" />
		<file src="**\*.cs" target="src\" exclude="obj\**\*.cs;bin\**\*.cs"/>
	</files>
</package>